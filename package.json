{
  "name": "vue-penetrate",
  "version": "0.1.1",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name a.js src/views/scroll.js",
    "out": "webpack --config webpack.m-build.js --mode=development --progress --debug --display-modules --sort-modules-by size",
    "out:r": "webpack --config webpack.m-build.js --mode=production --progress --debug --display-modules --sort-modules-by size",
    "babel": "babel ./src/views/scroll.js --out-file ./m-dist/penetrate-babel.js --source-maps"
  },
  "main": "./m-dist/penetrate.js",
  "files": [
    "m-dist"
  ],
  "dependencies": {
    "vue": "^2.5.21",
    "vue-penetrate": "^0.1.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  }
}
